pipeline {
    agent any       
    environment {
        ENV_URL = "pipeline.google.com"
    }                
    stages {
        stage('Stage One') {
            steps {
                sh '''
                echo Hello World
                echo Stage One Demo
                echo environment URL is ${ENV_URL}
                '''
            }
        }
        stage('Stage Two') {
            steps {
                sh "echo stage Two Demo"
            }
        }
        stage('Stage Three') {
            steps {
                sh "echo stage Three Demo"
            }
        }
    }
}
